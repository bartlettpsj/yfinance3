  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Download Chart Prices",
      "args": ["--symbol=SYM", "--start=2025-12-25",  "--end=2025-12-31", "--interval=1h"],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/download_chart_prices.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Analysis4",
      "args": ["--start=2024-01-01",  "--end=2025-12-31", "--interval=1h"],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/analysis4.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Download Symbols",
      "args": [],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/download_symbols.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Download All Symbol Prices",
      "args": ["--start=2024-01-01",  "--end=2025-12-31", "--interval=1d"],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/download_all_symbol_prices.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run current TS file",
      // "runtimeArgs": ["-r", "ts-node/register"],
      // "args": ["${file}"],
      "program": "${workspaceFolder}/${fileBasename}"
      // "cwd": "${workspaceFolder}/dist"
    }    
  ]
}
